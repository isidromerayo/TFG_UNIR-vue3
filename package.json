{
  "name": "frontend-vue",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test-headless": "vitest run",
    "test-headless-cc": "vitest run --coverage",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:component": "cypress run --component",
    "cypress:component:open": "cypress open --component",
    "cypress:component:coverage": "CYPRESS_COVERAGE=true cypress run --component && npx nyc report --reporter=lcov --reporter=text-summary",
    "cypress:component:coverage:open": "CYPRESS_COVERAGE=true cypress open --component",
    "cypress:e2e": "cypress run --e2e",
    "cypress:e2e:coverage": "CYPRESS_COVERAGE=true cypress run --e2e",
    "coverage:merge": "mkdir -p coverage/merged && npx lcov-result-merger 'coverage/**/lcov.info' coverage/merged/lcov.info",
    "coverage:verify": "node scripts/verify-coverage.cjs",
    "test:all": "pnpm test-headless && pnpm cypress:component",
    "test:all:ci": "pnpm test:coverage && pnpm cypress:component:coverage",
    "coverage:report": "echo 'Coverage report generated at coverage/index.html'",
    "security": "./scripts/security-check.sh",
    "security:audit": "pnpm audit",
    "security:outdated": "pnpm outdated"
  },
  "dependencies": {
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.13.4",
    "@cypress/vue": "^6.0.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@pinia/testing": "^1.0.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/cypress": "^1.1.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.14",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/eslint-plugin": "^1.6.4",
    "@vitest/ui": "^4.0.16",
    "@vue/compiler-sfc": "^3.5.17",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/test-utils": "^2.4.0-alpha.2",
    "@vue/tsconfig": "^0.8.0",
    "axios": "^1.10.0",
    "cypress": "^15.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "~10.5.1",
    "istanbul-lib-coverage": "^3.2.2",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lcov-result-merger": "^5.0.1",
    "npm-run-all2": "^8.0.4",
    "nyc": "^17.1.0",
    "prettier": "3.6.2",
    "sweetalert2": "^11.4.8",
    "typescript": "~5.9.2",
    "vite": "^6.3.5",
    "vite-plugin-istanbul": "^6.0.2",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^4.0.16",
    "vue-router": "^4.6.4",
    "vue-tsc": "^2.2.10",
    "vuex": "^4.1.0",
    "yup": "^1.6.1"
  },
  "pnpm": {
    "overrides": {
      "qs": "6.14.1",
      "js-yaml": "4.1.1"
    }
  }
}